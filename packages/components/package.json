{
  "name": "@yetuzi/vue3-query-components",
  "version": "1.4.5",
  "description": "Vue3查询页面组件库，基于Element Plus封装的表格查询组件 - 开箱即用",
  "type": "module",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./dist/style.css": "./dist/index.css",
    "./dist/index.css": "./dist/index.css"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE",
    "CHANGELOG.md"
  ],
  "scripts": {
    "dev": "node scripts/dev-watch.js",
    "build": "node scripts/build-optimized.js",
    "build:legacy": "node scripts/build.js",
    "build:patch": "node scripts/build.js patch",
    "build:minor": "node scripts/build.js minor",
    "build:major": "node scripts/build.js major",
    "publish:patch": "node scripts/publish.js patch",
    "publish:minor": "node scripts/publish.js minor",
    "publish:major": "node scripts/publish.js major",
    "type-check": "vue-tsc --noEmit --skipLibCheck",
    "test": "npm run build",
    "release": "standard-version",
    "release:patch": "standard-version --release-as patch",
    "release:minor": "standard-version --release-as minor",
    "release:major": "standard-version --release-as major",
    "release:dry": "standard-version --dry-run",
    "version": "node scripts/generate-version-info.js"
  },
  "keywords": [
    "vue3",
    "element-plus",
    "components",
    "typescript",
    "ui",
    "table",
    "form",
    "query",
    "search",
    "list-page",
    "data-table",
    "crud"
  ],
  "author": "yetuzi",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/yetuzi-open/vue3-query-components"
  },
  "bugs": {
    "url": "https://github.com/yetuzi-open/vue3-query-components/issues"
  },
  "homepage": "https://yetuzi-open.github.io/vue3-query-components",
  "peerDependencies": {
    "element-plus": "^2.11.5",
    "vue": "^3.4.0"
  },
  "dependencies": {
    "dayjs": "^1.11.18",
    "lodash-es": "^4.17.21",
    "vue-hooks-plus": "^2.4.1"
  },
  "devDependencies": {
    "@types/node": "^22.0.0",
    "@vitejs/plugin-vue": "^6.0.3",
    "@vue/tsconfig": "^0.8.0",
    "sass": "^1.97.0",
    "standard-version": "^9.5.0",
    "typescript": "~5.9.0",
    "unplugin-element-plus": "^0.11.2",
    "vite": "^6.4.1",
    "vite-plugin-dts": "^4.5.4",
    "vite-plugin-lib-inject-css": "^2.2.2",
    "vite-plugin-package-version": "^1.1.0",
    "vue-component-type-helpers": "^3.2.1",
    "vue-tsc": "^3.0.0"
  },
  "engines": {
    "node": "^20.19.0 || >=22.12.0"
  },
  "volta": {
    "node": "20.19.0"
  }
}
